<div class="page-header">
  <h1>Owners</h1>
</div>


<p><a href="/owners/new" class="btn btn-primary">Add a New Owner</a></p>


<table class="table table-hover">
  <thead>
    <tr>

      <th>Player</th>

      <th>User</th>

      <th>Created at</th>

      <th>Updated at</th>

      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
      <tr>

        <td><%= owner.player_id %></td>

        <td><%= owner.user_id %></td>

        <td><%= owner.created_at %></td>

        <td><%= owner.updated_at %></td>


        <td>
          <a href="/owners/<%= owner.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/owners/<%= owner.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_owner/<%= owner.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>

      </tr>
    <% end %>
  </tbody>
</table>

